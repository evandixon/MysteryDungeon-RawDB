@model IEnumerable<ProjectPokemon.Pokedex.Models.Games.Gen7.Gen7EggGroup>

<h2>Egg Groups</h2>

<table class="table table-striped table-bordered table-condensed">
    <tr>
        <th width="60%">
            Name
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <a href="@(Url.Action("Details", new { id = item.Name }))">@item.Name</a>
            </td>
        </tr>
    }

</table>