@model IEnumerable<ProjectPokemon.Pokedex.Models.Games.Gen7.Gen7PkmType>

<h2>Types</h2>

<table class="table table-striped table-bordered table-condensed">
    <tr>
        <th width="10%">
            ID
        </th>
        <th width="60%">
            Name
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.ID
            </td>
            <td>
                <a href="@Url.Action("Details", new { id = item.ID })">@item.Name</a>
            </td>
        </tr>
    }

</table>